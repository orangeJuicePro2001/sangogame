    "isCustomPhraseDesc": true,
    "customPhraseDescTS": "// 根据属性名称值编写自定义短语描述，如：\"我的短语是\"+attrs.xxx\n// 使用下拉框组件的变量attrs.xxx有对应的attrs.xxxItems的属性以便获得标签名集合\n// 获取自定义模块的数据名称的函数：getModuleDataName(moduleID:number,dataID:number):string;\n// 获取各种列表内数据名称的函数：getTypeDataListName(typeID: number, dataID: number):string;\n// -- typeID ：0-全局变量 1-全局开关 2-全局字符串 3-玩家变量 4-玩家开关 5-玩家字符串\n//           ：6-场景列表 7-场景对象模型列表 8-图块列表 9-行走图列表 10-行走图动作列表 11-立绘列表 12-立绘表情列表\n//           ：13-公共事件 14-对话框列表 15-动画列表 16-动画信号列表 17-界面列表 18-数据结构列表 19-自定义模块列表\n//           ：20-自定义命令列表 21-自定义行为列表 22-自动元件列表 23-行走图部件列表 24-立绘部件列表\nvar str = \"\";\nif (attrs.layerUseVar == 1) {\n    var layerStr: string = `[${attrs.layerVarID}-${getTypeDataListName(3, attrs.layerVarID)}]`;\n}\nelse {\n    layerStr = attrs.layer.toString();\n}\nstr += `当前地图图层${layerStr}：` + attrs.typeItems[attrs.type];\nif (attrs.type == 0) {\n    str += \"(\";\n    if (attrs.useVar) {\n        str += `[${attrs.xVarID}-${getTypeDataListName(3, attrs.xVarID)}],[${attrs.yVarID}-${getTypeDataListName(3, attrs.yVarID)}]`;\n    }\n    else {\n        str += `${attrs.x},${attrs.y}`;\n    }\n    str += \")\";\n}\nstr;",
    "customPhraseDescJS": "var str = \"\";\nif (attrs.layerUseVar == 1) {\n    var layerStr = \"[\" + attrs.layerVarID + \"-\" + getTypeDataListName(3, attrs.layerVarID) + \"]\";\n}\nelse {\n    layerStr = attrs.layer.toString();\n}\nstr += (\"\\u5F53\\u524D\\u5730\\u56FE\\u56FE\\u5C42\" + layerStr + \"\\uFF1A\") + attrs.typeItems[attrs.type];\nif (attrs.type == 0) {\n    str += \"(\";\n    if (attrs.useVar) {\n        str += \"[\" + attrs.xVarID + \"-\" + getTypeDataListName(3, attrs.xVarID) + \"],[\" + attrs.yVarID + \"-\" + getTypeDataListName(3, attrs.yVarID) + \"]\";\n    }\n    else {\n        str += attrs.x + \",\" + attrs.y;\n    }\n    str += \")\";\n}\nstr;\n"
}
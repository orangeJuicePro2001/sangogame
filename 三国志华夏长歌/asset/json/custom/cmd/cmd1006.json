    "isCustomPhraseDesc": true,
    "customPhraseDescTS": "// 根据属性名称值编写自定义短语描述，如：\"我的短语是\"+attrs.xxx\n// 使用下拉框组件的变量attrs.xxx有对应的attrs.xxxItems的属性以便获得标签名集合\n// 获取自定义模块的数据名称的函数：getModuleDataName(moduleID:number,dataID:number):string;\n// 获取各种列表内数据名称的函数：getTypeDataListName(typeID: number, dataID: number):string;\n// -- typeID ：0-全局变量 1-全局开关 2-全局字符串 3-玩家变量 4-玩家开关 5-玩家字符串\n//           ：6-场景列表 7-场景对象模型列表 8-图块列表 9-行走图列表 10-行走图动作列表 11-立绘列表 12-立绘表情列表\n//           ：13-公共事件 14-对话框列表 15-动画列表 16-动画信号列表 17-界面列表 18-数据结构列表 19-自定义模块列表\n//           ：20-自定义命令列表 21-自定义行为列表 22-自动元件列表 23-行走图部件列表 24-立绘部件列表\nvar str = \"\";\nif (attrs.aniUseVar) {\n    var aniName: string = `「${attrs.aniIDVarID}-${getTypeDataListName(3, attrs.aniIDVarID)}」`\n}\nelse {\n    aniName = `「${attrs.aniID}-${getTypeDataListName(15, attrs.aniID)}」`;\n}\n\nstr += `${aniName}显示在`;\nstr += attrs.useTypeItems[attrs.useType];\nif (attrs.useType == 0) {\n    str += \"-\" + attrs.soTypeItems[attrs.soType];\n    if (attrs.soType == 3) {\n        str += \":\";\n        if (attrs.useVar) {\n            str += ` [${attrs.soIndexVarID}-${getTypeDataListName(3, attrs.soIndexVarID)}]`;\n        }\n        else {\n            str += ` ${attrs.soIndex}号`;\n        }\n    }\n}\nelse if (attrs.useType == 1) {\n    str += attrs.isGrid ? \"-格子\" : \"\";\n    str += \"(\";\n    if (attrs.posUseVar) {\n        str += `[${attrs.xVarID}-${getTypeDataListName(3, attrs.xVarID)}],[${attrs.yVarID}-${getTypeDataListName(3, attrs.yVarID)}]`;\n    }\n    else {\n        str += `${attrs.x},${attrs.y}`;\n    }\n    str += \")\";\n    str += \" \" + attrs.layerItems[attrs.layer];\n}\nstr;",
    "customPhraseDescJS": "var str = \"\";\nif (attrs.aniUseVar) {\n    var aniName = \"\\u300C\" + attrs.aniIDVarID + \"-\" + getTypeDataListName(3, attrs.aniIDVarID) + \"\\u300D\";\n}\nelse {\n    aniName = \"\\u300C\" + attrs.aniID + \"-\" + getTypeDataListName(15, attrs.aniID) + \"\\u300D\";\n}\nstr += aniName + \"\\u663E\\u793A\\u5728\";\nstr += attrs.useTypeItems[attrs.useType];\nif (attrs.useType == 0) {\n    str += \"-\" + attrs.soTypeItems[attrs.soType];\n    if (attrs.soType == 3) {\n        str += \":\";\n        if (attrs.useVar) {\n            str += \" [\" + attrs.soIndexVarID + \"-\" + getTypeDataListName(3, attrs.soIndexVarID) + \"]\";\n        }\n        else {\n            str += \" \" + attrs.soIndex + \"\\u53F7\";\n        }\n    }\n}\nelse if (attrs.useType == 1) {\n    str += attrs.isGrid ? \"-格子\" : \"\";\n    str += \"(\";\n    if (attrs.posUseVar) {\n        str += \"[\" + attrs.xVarID + \"-\" + getTypeDataListName(3, attrs.xVarID) + \"],[\" + attrs.yVarID + \"-\" + getTypeDataListName(3, attrs.yVarID) + \"]\";\n    }\n    else {\n        str += attrs.x + \",\" + attrs.y;\n    }\n    str += \")\";\n    str += \" \" + attrs.layerItems[attrs.layer];\n}\nstr;\n",
    "compAutoOrder": false,
    "fastModifyAttribute": false
}